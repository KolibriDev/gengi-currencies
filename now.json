{
  "version": 2,
  "builds": [
    {"src": "currencies.js", "use": "@now/node"},
    { "src": "options.txt", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/.*",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/options.txt" }
  ]
}
